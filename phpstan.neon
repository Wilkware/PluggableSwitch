parameters:
    level: 6 # 0 is the loosest and 10 is the strictest
    phpVersion: 80200 # PHP 8.1.3

    # Analyse your module directory
    paths:
        - ./

    # Stubs with IPS functions
    scanDirectories:
        - %env.IPS_STUBS_PATH%

    # Exclude stubs self
    excludePaths:
        analyse:
            - %env.IPS_STUBS_PATH%/*
            
    # Supress call to an undefined method OR function not found
    ignoreErrors:

    # Supress ignore errors
    reportUnmatchedIgnoredErrors: false